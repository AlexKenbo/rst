          "verse": 8,
          "name": "2 Тимофею 4:8",
          "text": "\u0430 \u0442\u0435\u043f\u0435\u0440\u044c \u0433\u043e\u0442\u043e\u0432\u0438\u0442\u0441\u044f \u043c\u043d\u0435 \u0432\u0435\u043d\u0435\u0446 \u043f\u0440\u0430\u0432\u0434\u044b, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0434\u0430\u0441\u0442 \u043c\u043d\u0435 \u0413\u043e\u0441\u043f\u043e\u0434\u044c, \u043f\u0440\u0430\u0432\u0435\u0434\u043d\u044b\u0439 \u0421\u0443\u0434\u0438\u044f, \u0432 \u0434\u0435\u043d\u044c \u043e\u043d\u044b\u0439; \u0438 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043c\u043d\u0435, \u043d\u043e \u0438 \u0432\u0441\u0435\u043c, \u0432\u043e\u0437\u043b\u044e\u0431\u0438\u0432\u0448\u0438\u043c \u044f\u0432\u043b\u0435\u043d\u0438\u0435 \u0415\u0433\u043e."
        },
        {
          "chapter": 4,
          "verse": 9,
          "name": "2 Тимофею 4:9",
          "text": "\u041f\u043e\u0441\u0442\u0430\u0440\u0430\u0439\u0441\u044f \u043f\u0440\u0438\u0434\u0442\u0438 \u043a\u043e \u043c\u043d\u0435 \u0441\u043a\u043e\u0440\u043e."
        },
        {
          "chapter": 4,
          "verse": 10,
          "name": "2 Тимофею 4:10",
          "text": "\u0418\u0431\u043e \u0414\u0438\u043c\u0430\u0441 \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u043c\u0435\u043d\u044f, \u0432\u043e\u0437\u043b\u044e\u0431\u0438\u0432 \u043d\u044b\u043d\u0435\u0448\u043d\u0438\u0439 \u0432\u0435\u043a, \u0438 \u043f\u043e\u0448\u0435\u043b \u0432 \u0424\u0435\u0441\u0441\u0430\u043b\u043e\u043d\u0438\u043a\u0443, \u041a\u0440\u0438\u0441\u043a\u0435\u043d\u0442 \u0432 \u0413\u0430\u043b\u0430\u0442\u0438\u044e, \u0422\u0438\u0442 \u0432 \u0414\u0430\u043b\u043c\u0430\u0442\u0438\u044e; \u043e\u0434\u0438\u043d \u041b\u0443\u043a\u0430 \u0441\u043e \u043c\u043d\u043e\u044e."
        },
        {
          "chapter": 4,
          "verse": 11,
          "name": "2 Тимофею 4:11",
          "text": "\u041c\u0430\u0440\u043a\u0430 \u0432\u043e\u0437\u044c\u043c\u0438 \u0438 \u043f\u0440\u0438\u0432\u0435\u0434\u0438 \u0441 \u0441\u043e\u0431\u043e\u044e, \u0438\u0431\u043e \u043e\u043d \u043c\u043d\u0435 \u043d\u0443\u0436\u0435\u043d \u0434\u043b\u044f \u0441\u043b\u0443\u0436\u0435\u043d\u0438\u044f."
        },
        {
          "chapter": 4,
          "verse": 12,
          "name": "2 Тимофею 4:12",
          "text": "\u0422\u0438\u0445\u0438\u043a\u0430 \u044f \u043f\u043e\u0441\u043b\u0430\u043b \u0432 \u0415\u0444\u0435\u0441."
        },
        {
          "chapter": 4,
          "verse": 13,
          "name": "2 Тимофею 4:13",
          "text": "\u041a\u043e\u0433\u0434\u0430 \u043f\u043e\u0439\u0434\u0435\u0448\u044c, \u043f\u0440\u0438\u043d\u0435\u0441\u0438 \u0444\u0435\u043b\u043e\u043d\u044c, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u044f \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0432 \u0422\u0440\u043e\u0430\u0434\u0435 \u0443 \u041a\u0430\u0440\u043f\u0430, \u0438 \u043a\u043d\u0438\u0433\u0438, \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u043a\u043e\u0436\u0430\u043d\u044b\u0435."
        },
        {
          "chapter": 4,
          "verse": 14,
          "name": "2 Тимофею 4:14",
          "text": "\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u043c\u0435\u0434\u043d\u0438\u043a \u043c\u043d\u043e\u0433\u043e \u0441\u0434\u0435\u043b\u0430\u043b \u043c\u043d\u0435 \u0437\u043b\u0430. \u0414\u0430 \u0432\u043e\u0437\u0434\u0430\u0441\u0442 \u0435\u043c\u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u043f\u043e \u0434\u0435\u043b\u0430\u043c \u0435\u0433\u043e!"
        },
        {
          "chapter": 4,
          "verse": 15,
          "name": "2 Тимофею 4:15",
          "text": "\u0411\u0435\u0440\u0435\u0433\u0438\u0441\u044c \u0435\u0433\u043e \u0438 \u0442\u044b, \u0438\u0431\u043e \u043e\u043d \u0441\u0438\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0442\u0438\u0432\u0438\u043b\u0441\u044f \u043d\u0430\u0448\u0438\u043c \u0441\u043b\u043e\u0432\u0430\u043c."
        },
        {
          "chapter": 4,
          "verse": 16,
          "name": "2 Тимофею 4:16",
          "text": "\u041f\u0440\u0438 \u043f\u0435\u0440\u0432\u043e\u043c \u043c\u043e\u0435\u043c \u043e\u0442\u0432\u0435\u0442\u0435 \u043d\u0438\u043a\u043e\u0433\u043e \u043d\u0435 \u0431\u044b\u043b\u043e \u0441\u043e \u043c\u043d\u043e\u044e, \u043d\u043e \u0432\u0441\u0435 \u043c\u0435\u043d\u044f \u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0438. \u0414\u0430 \u043d\u0435 \u0432\u043c\u0435\u043d\u0438\u0442\u0441\u044f \u0438\u043c!"
        },
        {
          "chapter": 4,
          "verse": 17,
          "name": "2 Тимофею 4:17",
          "text": "\u0413\u043e\u0441\u043f\u043e\u0434\u044c \u0436\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u043b \u043c\u043d\u0435 \u0438 \u0443\u043a\u0440\u0435\u043f\u0438\u043b \u043c\u0435\u043d\u044f, \u0434\u0430\u0431\u044b \u0447\u0435\u0440\u0435\u0437 \u043c\u0435\u043d\u044f \u0443\u0442\u0432\u0435\u0440\u0434\u0438\u043b\u043e\u0441\u044c \u0431\u043b\u0430\u0433\u043e\u0432\u0435\u0441\u0442\u0438\u0435 \u0438 \u0443\u0441\u043b\u044b\u0448\u0430\u043b\u0438 \u0432\u0441\u0435 \u044f\u0437\u044b\u0447\u043d\u0438\u043a\u0438; \u0438 \u044f \u0438\u0437\u0431\u0430\u0432\u0438\u043b\u0441\u044f \u0438\u0437 \u043b\u044c\u0432\u0438\u043d\u044b\u0445 \u0447\u0435\u043b\u044e\u0441\u0442\u0435\u0439."
        },
        {
          "chapter": 4,
          "verse": 18,
          "name": "2 Тимофею 4:18",
          "text": "\u0418 \u0438\u0437\u0431\u0430\u0432\u0438\u0442 \u043c\u0435\u043d\u044f \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u043e\u0442 \u0432\u0441\u044f\u043a\u043e\u0433\u043e \u0437\u043b\u043e\u0433\u043e \u0434\u0435\u043b\u0430 \u0438 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0442 \u0434\u043b\u044f \u0421\u0432\u043e\u0435\u0433\u043e \u041d\u0435\u0431\u0435\u0441\u043d\u043e\u0433\u043e \u0426\u0430\u0440\u0441\u0442\u0432\u0430, \u0415\u043c\u0443 \u0441\u043b\u0430\u0432\u0430 \u0432\u043e \u0432\u0435\u043a\u0438 \u0432\u0435\u043a\u043e\u0432. \u0410\u043c\u0438\u043d\u044c."
        },
        {
          "chapter": 4,
          "verse": 19,
          "name": "2 Тимофею 4:19",
          "text": "\u041f\u0440\u0438\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0439 \u041f\u0440\u0438\u0441\u043a\u0438\u043b\u043b\u0443 \u0438 \u0410\u043a\u0438\u043b\u0443 \u0438 \u0434\u043e\u043c \u041e\u043d\u0438\u0441\u0438\u0444\u043e\u0440\u043e\u0432."
        },
        {
          "chapter": 4,
          "verse": 20,
          "name": "2 Тимофею 4:20",
          "text": "\u0415\u0440\u0430\u0441\u0442 \u043e\u0441\u0442\u0430\u043b\u0441\u044f \u0432 \u041a\u043e\u0440\u0438\u043d\u0444\u0435; \u0422\u0440\u043e\u0444\u0438\u043c\u0430 \u0436\u0435 \u044f \u043e\u0441\u0442\u0430\u0432\u0438\u043b \u0431\u043e\u043b\u044c\u043d\u043e\u0433\u043e \u0432 \u041c\u0438\u043b\u0438\u0442\u0435."
        },
        {
          "chapter": 4,
          "verse": 21,
          "name": "2 Тимофею 4:21",
          "text": "\u041f\u043e\u0441\u0442\u0430\u0440\u0430\u0439\u0441\u044f \u043f\u0440\u0438\u0434\u0442\u0438 \u0434\u043e \u0437\u0438\u043c\u044b. \u041f\u0440\u0438\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u0442\u0435\u0431\u044f \u0415\u0432\u0432\u0443\u043b, \u0438 \u041f\u0443\u0434, \u0438 \u041b\u0438\u043d, \u0438 \u041a\u043b\u0430\u0432\u0434\u0438\u044f, \u0438 \u0432\u0441\u0435 \u0431\u0440\u0430\u0442\u0438\u044f."
        },
        {
          "chapter": 4,
          "verse": 22,
          "name": "2 Тимофею 4:22",
          "text": "\u0413\u043e\u0441\u043f\u043e\u0434\u044c \u0418\u0438\u0441\u0443\u0441 \u0425\u0440\u0438\u0441\u0442\u043e\u0441 \u0441\u043e \u0434\u0443\u0445\u043e\u043c \u0442\u0432\u043e\u0438\u043c. \u0411\u043b\u0430\u0433\u043e\u0434\u0430\u0442\u044c \u0441 \u0432\u0430\u043c\u0438. \u0410\u043c\u0438\u043d\u044c.  "
        }
      ]
    }
  ]
}
