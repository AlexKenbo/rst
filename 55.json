  "name": "2 Тимофею",
  "chapters": [
    {
      "chapter": 1,
      "name": "2 Тимофею 1",
      "verses": [
        {
          "chapter": 1,
          "verse": 1,
          "name": "2 Тимофею 1:1",
          "text": "\u041f\u0430\u0432\u0435\u043b, \u0432\u043e\u043b\u0435\u044e \u0411\u043e\u0436\u0438\u0435\u044e \u0410\u043f\u043e\u0441\u0442\u043e\u043b \u0418\u0438\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430, \u043f\u043e \u043e\u0431\u0435\u0442\u043e\u0432\u0430\u043d\u0438\u044e \u0436\u0438\u0437\u043d\u0438 \u0432\u043e \u0425\u0440\u0438\u0441\u0442\u0435 \u0418\u0438\u0441\u0443\u0441\u0435,"
        },
        {
          "chapter": 1,
          "verse": 2,
          "name": "2 Тимофею 1:2",
          "text": "\u0422\u0438\u043c\u043e\u0444\u0435\u044e, \u0432\u043e\u0437\u043b\u044e\u0431\u043b\u0435\u043d\u043d\u043e\u043c\u0443 \u0441\u044b\u043d\u0443: \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0442\u044c, \u043c\u0438\u043b\u043e\u0441\u0442\u044c, \u043c\u0438\u0440 \u043e\u0442 \u0411\u043e\u0433\u0430 \u041e\u0442\u0446\u0430 \u0438 \u0425\u0440\u0438\u0441\u0442\u0430 \u0418\u0438\u0441\u0443\u0441\u0430, \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430\u0448\u0435\u0433\u043e."
        },
        {
          "chapter": 1,
          "verse": 3,
          "name": "2 Тимофею 1:3",
          "text": "\u0411\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044e \u0411\u043e\u0433\u0430, \u041a\u043e\u0442\u043e\u0440\u043e\u043c\u0443 \u0441\u043b\u0443\u0436\u0443 \u043e\u0442 \u043f\u0440\u0430\u0440\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439 \u0441 \u0447\u0438\u0441\u0442\u043e\u044e \u0441\u043e\u0432\u0435\u0441\u0442\u044c\u044e, \u0447\u0442\u043e \u043d\u0435\u043f\u0440\u0435\u0441\u0442\u0430\u043d\u043d\u043e \u0432\u0441\u043f\u043e\u043c\u0438\u043d\u0430\u044e \u043e \u0442\u0435\u0431\u0435 \u0432 \u043c\u043e\u043b\u0438\u0442\u0432\u0430\u0445 \u043c\u043e\u0438\u0445 \u0434\u043d\u0435\u043c \u0438 \u043d\u043e\u0447\u044c\u044e,"
        },
        {
          "chapter": 1,
          "verse": 4,
          "name": "2 Тимофею 1:4",
          "text": "\u0438 \u0436\u0435\u043b\u0430\u044e \u0432\u0438\u0434\u0435\u0442\u044c \u0442\u0435\u0431\u044f, \u0432\u0441\u043f\u043e\u043c\u0438\u043d\u0430\u044f \u043e \u0441\u043b\u0435\u0437\u0430\u0445 \u0442\u0432\u043e\u0438\u0445, \u0434\u0430\u0431\u044b \u043c\u043d\u0435 \u0438\u0441\u043f\u043e\u043b\u043d\u0438\u0442\u044c\u0441\u044f \u0440\u0430\u0434\u043e\u0441\u0442\u0438,"
        },
        {
          "chapter": 1,
          "verse": 5,
          "name": "2 Тимофею 1:5",
          "text": "\u043f\u0440\u0438\u0432\u043e\u0434\u044f \u043d\u0430 \u043f\u0430\u043c\u044f\u0442\u044c \u043d\u0435\u043b\u0438\u0446\u0435\u043c\u0435\u0440\u043d\u0443\u044e \u0432\u0435\u0440\u0443 \u0442\u0432\u043e\u044e, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043f\u0440\u0435\u0436\u0434\u0435 \u043e\u0431\u0438\u0442\u0430\u043b\u0430 \u0432 \u0431\u0430\u0431\u043a\u0435 \u0442\u0432\u043e\u0435\u0439 \u041b\u043e\u0438\u0434\u0435 \u0438 \u043c\u0430\u0442\u0435\u0440\u0438 \u0442\u0432\u043e\u0435\u0439 \u0415\u0432\u043d\u0438\u043a\u0435; \u0443\u0432\u0435\u0440\u0435\u043d, \u0447\u0442\u043e \u043e\u043d\u0430 \u0438 \u0432 \u0442\u0435\u0431\u0435."
        },
        {
          "chapter": 1,
          "verse": 6,
          "name": "2 Тимофею 1:6",
          "text": "\u041f\u043e \u0441\u0435\u0439 \u043f\u0440\u0438\u0447\u0438\u043d\u0435 \u043d\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u044e \u0442\u0435\u0431\u0435 \u0432\u043e\u0437\u0433\u0440\u0435\u0432\u0430\u0442\u044c \u0434\u0430\u0440 \u0411\u043e\u0436\u0438\u0439, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432 \u0442\u0435\u0431\u0435 \u0447\u0435\u0440\u0435\u0437 \u043c\u043e\u0435 \u0440\u0443\u043a\u043e\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435;"
        },
        {
          "chapter": 1,
          "verse": 7,
          "name": "2 Тимофею 1:7",
          "text": "\u0438\u0431\u043e \u0434\u0430\u043b \u043d\u0430\u043c \u0411\u043e\u0433 \u0434\u0443\u0445\u0430 \u043d\u0435 \u0431\u043e\u044f\u0437\u043d\u0438, \u043d\u043e \u0441\u0438\u043b\u044b \u0438 \u043b\u044e\u0431\u0432\u0438 \u0438 \u0446\u0435\u043b\u043e\u043c\u0443\u0434\u0440\u0438\u044f."
        },
        {
          "chapter": 1,
          "verse": 8,
          "name": "2 Тимофею 1:8",
          "text": "\u0418\u0442\u0430\u043a, \u043d\u0435 \u0441\u0442\u044b\u0434\u0438\u0441\u044c \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u043d\u0430\u0448\u0435\u0433\u043e \u0418\u0438\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430, \u043d\u0438 \u043c\u0435\u043d\u044f, \u0443\u0437\u043d\u0438\u043a\u0430 \u0415\u0433\u043e; \u043d\u043e \u0441\u0442\u0440\u0430\u0434\u0430\u0439 \u0441 \u0431\u043b\u0430\u0433\u043e\u0432\u0435\u0441\u0442\u0438\u0435\u043c \u0425\u0440\u0438\u0441\u0442\u043e\u0432\u044b\u043c \u0441\u0438\u043b\u043e\u044e \u0411\u043e\u0433\u0430,"
        },
        {
          "chapter": 1,
          "verse": 9,
          "name": "2 Тимофею 1:9",
          "text": "\u0441\u043f\u0430\u0441\u0448\u0435\u0433\u043e \u043d\u0430\u0441 \u0438 \u043f\u0440\u0438\u0437\u0432\u0430\u0432\u0448\u0435\u0433\u043e \u0437\u0432\u0430\u043d\u0438\u0435\u043c \u0441\u0432\u044f\u0442\u044b\u043c, \u043d\u0435 \u043f\u043e \u0434\u0435\u043b\u0430\u043c \u043d\u0430\u0448\u0438\u043c, \u043d\u043e \u043f\u043e \u0421\u0432\u043e\u0435\u043c\u0443 \u0438\u0437\u0432\u043e\u043b\u0435\u043d\u0438\u044e \u0438 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0442\u0438, \u0434\u0430\u043d\u043d\u043e\u0439 \u043d\u0430\u043c \u0432\u043e \u0425\u0440\u0438\u0441\u0442\u0435 \u0418\u0438\u0441\u0443\u0441\u0435 \u043f\u0440\u0435\u0436\u0434\u0435 \u0432\u0435\u043a\u043e\u0432\u044b\u0445 \u0432\u0440\u0435\u043c\u0435\u043d,"
        },
        {
          "chapter": 1,
          "verse": 10,
          "name": "2 Тимофею 1:10",
          "text": "\u043e\u0442\u043a\u0440\u044b\u0432\u0448\u0435\u0439\u0441\u044f \u0436\u0435 \u043d\u044b\u043d\u0435 \u044f\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u0421\u043f\u0430\u0441\u0438\u0442\u0435\u043b\u044f \u043d\u0430\u0448\u0435\u0433\u043e \u0418\u0438\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430, \u0440\u0430\u0437\u0440\u0443\u0448\u0438\u0432\u0448\u0435\u0433\u043e \u0441\u043c\u0435\u0440\u0442\u044c \u0438 \u044f\u0432\u0438\u0432\u0448\u0435\u0433\u043e \u0436\u0438\u0437\u043d\u044c \u0438 \u043d\u0435\u0442\u043b\u0435\u043d\u0438\u0435 \u0447\u0435\u0440\u0435\u0437 \u0431\u043b\u0430\u0433\u043e\u0432\u0435\u0441\u0442\u0438\u0435,"
        },
        {
          "chapter": 1,
          "verse": 11,
          "name": "2 Тимофею 1:11",
          "text": "\u0434\u043b\u044f \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u044f \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d \u043f\u0440\u043e\u043f\u043e\u0432\u0435\u0434\u043d\u0438\u043a\u043e\u043c \u0438 \u0410\u043f\u043e\u0441\u0442\u043e\u043b\u043e\u043c \u0438 \u0443\u0447\u0438\u0442\u0435\u043b\u0435\u043c \u044f\u0437\u044b\u0447\u043d\u0438\u043a\u043e\u0432."
        },
        {
          "chapter": 1,
          "verse": 12,
          "name": "2 Тимофею 1:12",
          "text": "\u041f\u043e \u0441\u0435\u0439 \u043f\u0440\u0438\u0447\u0438\u043d\u0435 \u044f \u0438 \u0441\u0442\u0440\u0430\u0434\u0430\u044e \u0442\u0430\u043a; \u043d\u043e \u043d\u0435 \u0441\u0442\u044b\u0436\u0443\u0441\u044c. \u0418\u0431\u043e \u044f \u0437\u043d\u0430\u044e, \u0432 \u041a\u043e\u0433\u043e \u0443\u0432\u0435\u0440\u043e\u0432\u0430\u043b, \u0438 \u0443\u0432\u0435\u0440\u0435\u043d, \u0447\u0442\u043e \u041e\u043d \u0441\u0438\u043b\u0435\u043d \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u044c \u0437\u0430\u043b\u043e\u0433 \u043c\u043e\u0439 \u043d\u0430 \u043e\u043d\u044b\u0439 \u0434\u0435\u043d\u044c."
        },
        {
          "chapter": 1,
          "verse": 13,
          "name": "2 Тимофею 1:13",
          "text": "\u0414\u0435\u0440\u0436\u0438\u0441\u044c \u043e\u0431\u0440\u0430\u0437\u0446\u0430 \u0437\u0434\u0440\u0430\u0432\u043e\u0433\u043e \u0443\u0447\u0435\u043d\u0438\u044f, \u043a\u043e\u0442\u043e\u0440\u043e\u0435 \u0442\u044b \u0441\u043b\u044b\u0448\u0430\u043b \u043e\u0442 \u043c\u0435\u043d\u044f, \u0441 \u0432\u0435\u0440\u043e\u044e \u0438 \u043b\u044e\u0431\u043e\u0432\u044c\u044e \u0432\u043e \u0425\u0440\u0438\u0441\u0442\u0435 \u0418\u0438\u0441\u0443\u0441\u0435."
        },
        {
          "chapter": 1,
          "verse": 14,
          "name": "2 Тимофею 1:14",
          "text": "\u0425\u0440\u0430\u043d\u0438 \u0434\u043e\u0431\u0440\u044b\u0439 \u0437\u0430\u043b\u043e\u0433 \u0414\u0443\u0445\u043e\u043c \u0421\u0432\u044f\u0442\u044b\u043c, \u0436\u0438\u0432\u0443\u0449\u0438\u043c \u0432 \u043d\u0430\u0441."
        },
        {
          "chapter": 1,
          "verse": 15,
          "name": "2 Тимофею 1:15",
          "text": "\u0422\u044b \u0437\u043d\u0430\u0435\u0448\u044c, \u0447\u0442\u043e \u0432\u0441\u0435 \u0410\u0441\u0438\u0439\u0441\u043a\u0438\u0435 \u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0438 \u043c\u0435\u043d\u044f; \u0432 \u0447\u0438\u0441\u043b\u0435 \u0438\u0445 \u0424\u0438\u0433\u0435\u043b\u043b \u0438 \u0415\u0440\u043c\u043e\u0433\u0435\u043d."
        },
        {
          "chapter": 1,
          "verse": 16,
          "name": "2 Тимофею 1:16",
          "text": "\u0414\u0430 \u0434\u0430\u0441\u0442 \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u043c\u0438\u043b\u043e\u0441\u0442\u044c \u0434\u043e\u043c\u0443 \u041e\u043d\u0438\u0441\u0438\u0444\u043e\u0440\u0430 \u0437\u0430 \u0442\u043e, \u0447\u0442\u043e \u043e\u043d \u043c\u043d\u043e\u0433\u043e\u043a\u0440\u0430\u0442\u043d\u043e \u043f\u043e\u043a\u043e\u0438\u043b \u043c\u0435\u043d\u044f \u0438 \u043d\u0435 \u0441\u0442\u044b\u0434\u0438\u043b\u0441\u044f \u0443\u0437 \u043c\u043e\u0438\u0445,"
        },
        {
          "chapter": 1,
          "verse": 17,
          "name": "2 Тимофею 1:17",
          "text": "\u043d\u043e, \u0431\u044b\u0432 \u0432 \u0420\u0438\u043c\u0435, \u0441 \u0432\u0435\u043b\u0438\u043a\u0438\u043c \u0442\u0449\u0430\u043d\u0438\u0435\u043c \u0438\u0441\u043a\u0430\u043b \u043c\u0435\u043d\u044f \u0438 \u043d\u0430\u0448\u0435\u043b."
        },
        {
          "chapter": 1,
          "verse": 18,
          "name": "2 Тимофею 1:18",
          "text": "\u0414\u0430 \u0434\u0430\u0441\u0442 \u0435\u043c\u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043c\u0438\u043b\u043e\u0441\u0442\u044c \u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0432 \u043e\u043d\u044b\u0439 \u0434\u0435\u043d\u044c; \u0430 \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043e\u043d \u0441\u043b\u0443\u0436\u0438\u043b \u043c\u043d\u0435 \u0432 \u0415\u0444\u0435\u0441\u0435, \u0442\u044b \u043b\u0443\u0447\u0448\u0435 \u0437\u043d\u0430\u0435\u0448\u044c. "
        }
      ]
    },
    {
      "chapter": 2,
      "name": "2 Тимофею 2",
      "verses": [
        {
          "chapter": 2,
          "verse": 1,
          "name": "2 Тимофею 2:1",
          "text": "\u0418\u0442\u0430\u043a \u0443\u043a\u0440\u0435\u043f\u043b\u044f\u0439\u0441\u044f, \u0441\u044b\u043d \u043c\u043e\u0439, \u0432 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0442\u0438 \u0425\u0440\u0438\u0441\u0442\u043e\u043c \u0418\u0438\u0441\u0443\u0441\u043e\u043c,"
        },
        {
          "chapter": 2,
          "verse": 2,
          "name": "2 Тимофею 2:2",
          "text": "\u0438 \u0447\u0442\u043e \u0441\u043b\u044b\u0448\u0430\u043b \u043e\u0442 \u043c\u0435\u043d\u044f \u043f\u0440\u0438 \u043c\u043d\u043e\u0433\u0438\u0445 \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u044f\u0445, \u0442\u043e \u043f\u0435\u0440\u0435\u0434\u0430\u0439 \u0432\u0435\u0440\u043d\u044b\u043c \u043b\u044e\u0434\u044f\u043c, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0431\u044b\u043b\u0438 \u0431\u044b \u0441\u043f\u043e\u0441\u043e\u0431\u043d\u044b \u0438 \u0434\u0440\u0443\u0433\u0438\u0445 \u043d\u0430\u0443\u0447\u0438\u0442\u044c."
        },
        {
          "chapter": 2,
          "verse": 3,
          "name": "2 Тимофею 2:3",
          "text": "\u0418\u0442\u0430\u043a \u043f\u0435\u0440\u0435\u043d\u043e\u0441\u0438 \u0441\u0442\u0440\u0430\u0434\u0430\u043d\u0438\u044f, \u043a\u0430\u043a \u0434\u043e\u0431\u0440\u044b\u0439 \u0432\u043e\u0438\u043d \u0418\u0438\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430."
        },
        {
          "chapter": 2,
          "verse": 4,
          "name": "2 Тимофею 2:4",
          "text": "\u041d\u0438\u043a\u0430\u043a\u043e\u0439 \u0432\u043e\u0438\u043d \u043d\u0435 \u0441\u0432\u044f\u0437\u044b\u0432\u0430\u0435\u0442 \u0441\u0435\u0431\u044f \u0434\u0435\u043b\u0430\u043c\u0438 \u0436\u0438\u0442\u0435\u0439\u0441\u043a\u0438\u043c\u0438, \u0447\u0442\u043e\u0431\u044b \u0443\u0433\u043e\u0434\u0438\u0442\u044c \u0432\u043e\u0435\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a\u0443."
        },
        {
          "chapter": 2,
          "verse": 5,
          "name": "2 Тимофею 2:5",
          "text": "\u0415\u0441\u043b\u0438 \u0436\u0435 \u043a\u0442\u043e \u0438 \u043f\u043e\u0434\u0432\u0438\u0437\u0430\u0435\u0442\u0441\u044f, \u043d\u0435 \u0443\u0432\u0435\u043d\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f, \u0435\u0441\u043b\u0438 \u043d\u0435\u0437\u0430\u043a\u043e\u043d\u043d\u043e \u0431\u0443\u0434\u0435\u0442 \u043f\u043e\u0434\u0432\u0438\u0437\u0430\u0442\u044c\u0441\u044f."
        },
        {
          "chapter": 2,
          "verse": 6,
          "name": "2 Тимофею 2:6",
          "text": "\u0422\u0440\u0443\u0434\u044f\u0449\u0435\u043c\u0443\u0441\u044f \u0437\u0435\u043c\u043b\u0435\u0434\u0435\u043b\u044c\u0446\u0443 \u043f\u0435\u0440\u0432\u043e\u043c\u0443 \u0434\u043e\u043b\u0436\u043d\u043e \u0432\u043a\u0443\u0441\u0438\u0442\u044c \u043e\u0442 \u043f\u043b\u043e\u0434\u043e\u0432."
        },
        {
          "chapter": 2,
          "verse": 7,
          "name": "2 Тимофею 2:7",
          "text": "\u0420\u0430\u0437\u0443\u043c\u0435\u0439, \u0447\u0442\u043e \u044f \u0433\u043e\u0432\u043e\u0440\u044e. \u0414\u0430 \u0434\u0430\u0441\u0442 \u0442\u0435\u0431\u0435 \u0413\u043e\u0441\u043f\u043e\u0434\u044c \u0440\u0430\u0437\u0443\u043c\u0435\u043d\u0438\u0435 \u0432\u043e \u0432\u0441\u0435\u043c."
        },
        {
          "chapter": 2,
          "verse": 8,
          "name": "2 Тимофею 2:8",
          "text": "\u041f\u043e\u043c\u043d\u0438 \u0413\u043e\u0441\u043f\u043e\u0434\u0430 \u0418\u0438\u0441\u0443\u0441\u0430 \u0425\u0440\u0438\u0441\u0442\u0430 \u043e\u0442 \u0441\u0435\u043c\u0435\u043d\u0438 \u0414\u0430\u0432\u0438\u0434\u043e\u0432\u0430, \u0432\u043e\u0441\u043a\u0440\u0435\u0441\u0448\u0435\u0433\u043e \u0438\u0437 \u043c\u0435\u0440\u0442\u0432\u044b\u0445, \u043f\u043e \u0431\u043b\u0430\u0433\u043e\u0432\u0435\u0441\u0442\u0432\u043e\u0432\u0430\u043d\u0438\u044e \u043c\u043e\u0435\u043c\u0443,"
        },
        {
          "chapter": 2,
          "verse": 9,
          "name": "2 Тимофею 2:9",
          "text": "\u0437\u0430 \u043a\u043e\u0442\u043e\u0440\u043e\u0435 \u044f \u0441\u0442\u0440\u0430\u0434\u0430\u044e \u0434\u0430\u0436\u0435 \u0434\u043e \u0443\u0437, \u043a\u0430\u043a \u0437\u043b\u043e\u0434\u0435\u0439; \u043d\u043e \u0434\u043b\u044f \u0441\u043b\u043e\u0432\u0430 \u0411\u043e\u0436\u0438\u044f \u043d\u0435\u0442 \u0443\u0437."
        },
        {
          "chapter": 2,
          "verse": 10,
          "name": "2 Тимофею 2:10",
          "text": "\u041f\u043e\u0441\u0435\u043c\u0443 \u044f \u0432\u0441\u0435 \u0442\u0435\u0440\u043f\u043b\u044e \u0440\u0430\u0434\u0438 \u0438\u0437\u0431\u0440\u0430\u043d\u043d\u044b\u0445, \u0434\u0430\u0431\u044b \u0438 \u043e\u043d\u0438 \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0438 \u0441\u043f\u0430\u0441\u0435\u043d\u0438\u0435 \u0432\u043e \u0425\u0440\u0438\u0441\u0442\u0435 \u0418\u0438\u0441\u0443\u0441\u0435 \u0441 \u0432\u0435\u0447\u043d\u043e\u044e \u0441\u043b\u0430\u0432\u043e\u044e."
        },
        {
          "chapter": 2,
          "verse": 11,
          "name": "2 Тимофею 2:11",
          "text": "\u0412\u0435\u0440\u043d\u043e \u0441\u043b\u043e\u0432\u043e: \u0435\u0441\u043b\u0438 \u043c\u044b \u0441 \u041d\u0438\u043c \u0443\u043c\u0435\u0440\u043b\u0438, \u0442\u043e \u0441 \u041d\u0438\u043c \u0438 \u043e\u0436\u0438\u0432\u0435\u043c;"
        },
        {
          "chapter": 2,
          "verse": 12,
          "name": "2 Тимофею 2:12",
          "text": "\u0435\u0441\u043b\u0438 \u0442\u0435\u0440\u043f\u0438\u043c, \u0442\u043e \u0441 \u041d\u0438\u043c \u0438 \u0446\u0430\u0440\u0441\u0442\u0432\u043e\u0432\u0430\u0442\u044c \u0431\u0443\u0434\u0435\u043c; \u0435\u0441\u043b\u0438 \u043e\u0442\u0440\u0435\u0447\u0435\u043c\u0441\u044f, \u0438 \u041e\u043d \u043e\u0442\u0440\u0435\u0447\u0435\u0442\u0441\u044f \u043e\u0442 \u043d\u0430\u0441;"
        },
        {
          "chapter": 2,
          "verse": 13,
          "name": "2 Тимофею 2:13",
          "text": "\u0435\u0441\u043b\u0438 \u043c\u044b \u043d\u0435\u0432\u0435\u0440\u043d\u044b, \u041e\u043d \u043f\u0440\u0435\u0431\u044b\u0432\u0430\u0435\u0442 \u0432\u0435\u0440\u0435\u043d, \u0438\u0431\u043e \u0421\u0435\u0431\u044f \u043e\u0442\u0440\u0435\u0447\u044c\u0441\u044f \u043d\u0435 \u043c\u043e\u0436\u0435\u0442."
