        },
        {
          "chapter": 3,
          "verse": 3,
          "name": "Иона 3:3",
          "text": "\u0418 \u0432\u0441\u0442\u0430\u043b \u0418\u043e\u043d\u0430 \u0438 \u043f\u043e\u0448\u0435\u043b \u0432 \u041d\u0438\u043d\u0435\u0432\u0438\u044e, \u043f\u043e \u0441\u043b\u043e\u0432\u0443 \u0413\u043e\u0441\u043f\u043e\u0434\u043d\u044e; \u041d\u0438\u043d\u0435\u0432\u0438\u044f \u0436\u0435 \u0431\u044b\u043b\u0430 \u0433\u043e\u0440\u043e\u0434 \u0432\u0435\u043b\u0438\u043a\u0438\u0439 \u0443 \u0411\u043e\u0433\u0430, \u043d\u0430 \u0442\u0440\u0438 \u0434\u043d\u044f \u0445\u043e\u0434\u044c\u0431\u044b."
        },
        {
          "chapter": 3,
          "verse": 4,
          "name": "Иона 3:4",
          "text": "\u0418 \u043d\u0430\u0447\u0430\u043b \u0418\u043e\u043d\u0430 \u0445\u043e\u0434\u0438\u0442\u044c \u043f\u043e \u0433\u043e\u0440\u043e\u0434\u0443, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u043c\u043e\u0436\u043d\u043e \u043f\u0440\u043e\u0439\u0442\u0438 \u0432 \u043e\u0434\u0438\u043d \u0434\u0435\u043d\u044c, \u0438 \u043f\u0440\u043e\u043f\u043e\u0432\u0435\u0434\u044b\u0432\u0430\u043b, \u0433\u043e\u0432\u043e\u0440\u044f: \u0435\u0449\u0435 \u0441\u043e\u0440\u043e\u043a \u0434\u043d\u0435\u0439 \u0438 \u041d\u0438\u043d\u0435\u0432\u0438\u044f \u0431\u0443\u0434\u0435\u0442 \u0440\u0430\u0437\u0440\u0443\u0448\u0435\u043d\u0430!"
        },
        {
          "chapter": 3,
          "verse": 5,
          "name": "Иона 3:5",
          "text": "\u0418 \u043f\u043e\u0432\u0435\u0440\u0438\u043b\u0438 \u041d\u0438\u043d\u0435\u0432\u0438\u0442\u044f\u043d\u0435 \u0411\u043e\u0433\u0443, \u0438 \u043e\u0431\u044a\u044f\u0432\u0438\u043b\u0438 \u043f\u043e\u0441\u0442, \u0438 \u043e\u0434\u0435\u043b\u0438\u0441\u044c \u0432\u043e \u0432\u0440\u0435\u0442\u0438\u0449\u0430, \u043e\u0442 \u0431\u043e\u043b\u044c\u0448\u043e\u0433\u043e \u0438\u0437 \u043d\u0438\u0445 \u0434\u043e \u043c\u0430\u043b\u043e\u0433\u043e."
        },
        {
          "chapter": 3,
          "verse": 6,
          "name": "Иона 3:6",
          "text": "\u042d\u0442\u043e \u0441\u043b\u043e\u0432\u043e \u0434\u043e\u0448\u043b\u043e \u0434\u043e \u0446\u0430\u0440\u044f \u041d\u0438\u043d\u0435\u0432\u0438\u0438, \u0438 \u043e\u043d \u0432\u0441\u0442\u0430\u043b \u0441 \u043f\u0440\u0435\u0441\u0442\u043e\u043b\u0430 \u0441\u0432\u043e\u0435\u0433\u043e, \u0438 \u0441\u043d\u044f\u043b \u0441 \u0441\u0435\u0431\u044f \u0446\u0430\u0440\u0441\u043a\u043e\u0435 \u043e\u0431\u043b\u0430\u0447\u0435\u043d\u0438\u0435 \u0441\u0432\u043e\u0435, \u0438 \u043e\u0434\u0435\u043b\u0441\u044f \u0432\u043e \u0432\u0440\u0435\u0442\u0438\u0449\u0435, \u0438 \u0441\u0435\u043b \u043d\u0430 \u043f\u0435\u043f\u043b\u0435,"
        },
